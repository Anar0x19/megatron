drop table if exists mpl;
create table if not exists mpl
(

    ip varbinary(100),
    port varbinary(100),
    country varbinary(100),
    reliability varbinary(100)
);
LOAD DATA INFILE '/tmp/megaproxylist.csv' INTO TABLE mpl
  FIELDS TERMINATED BY ';' ENCLOSED BY '"'
  LINES TERMINATED BY '\n'
  IGNORE 1 LINES;
  
